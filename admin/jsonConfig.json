                        "pt": "Validar configuração",
                        "nl": "Validate Configuratie",
                        "fr": "Valider la configuration",
                        "it": "Configurazione valida",
                        "es": "Validate Configuración",
                        "pl": "Konflikt Validida",
                        "uk": "Налаштування",
